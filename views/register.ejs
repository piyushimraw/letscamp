<% include partials/header%>
<div class="container main-content">
    <h3>Register New User</h3>
        <form action="/register" method="POST" >
        
            <div class="form-group">
                <label for="formGroupInputUserName">Username</label>
                <input type="text" name="username" id="formGroupInputUserName" class="form-control">
            </div>
            <div class="form-group">
                <label for="formGroupPassword">Password</label>
                <input type="password" name="password" id="formGroupPassword" class="form-control">
            </div>
    
            <button type="submit" class="btn btn-primary">Submit</button>
    
    
        </form>

</div>


<% include partials/footer%>